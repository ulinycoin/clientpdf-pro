{
  "name": "clientpdf-pro",
  "private": true,
  "version": "0.1.0",
  "description": "ðŸš€ Modern client-side PDF converter with interactive UI and atomic design system",
  "type": "module",
  "homepage": "https://github.com/ulinycoin/clientpdf-pro#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:analyze": "tsc --noEmit && vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "vercel-build": "npm run build",
    "analyze": "node scripts/analyze-bundle.js",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist",
    "test": "echo \"Tests coming soon\" && exit 0",
    "test:type": "npm run type-check",
    "clean": "rm -rf dist node_modules/.vite",
    "postinstall": "echo \"âœ… Dependencies installed successfully. Run 'npm run dev' to start development.\""
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
