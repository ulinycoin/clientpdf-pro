{
  "buildCommand": "npm run build:full",
  "outputDirectory": "dist",
  "trailingSlash": false,
  "routes": [
    {
      "src": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8",
        "Cache-Control": "public, max-age=3600"
      },
      "dest": "/robots.txt"
    },
    {
      "src": "/((?!api/.*|.*\\.html|.*\\.txt|.*\\.xml|.*\\.ico|.*\\.png|.*\\.jpg|.*\\.gif|.*\\.svg|.*\\.css|.*\\.js|.*\\.woff|.*\\.woff2|sitemap.*).*)",
      "dest": "/index.html"
    }
  ]
}