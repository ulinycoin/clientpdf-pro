        max-width: 1200px;
        margin: 64px auto 0;
      }
  
      .tool-group {
        margin-bottom: 80px;
      }
  
      .group-header {
        display: flex;
        align-items: center;
        gap: 20px;
        margin-bottom: 40px;
      }
  
      .group-label {
