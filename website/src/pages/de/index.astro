        transition: opacity 0.5s ease;
        pointer-events: none;
      }
  
      .tool-card:hover {
        transform: scale(1.02);
        border-color: rgba(255, 255, 255, 0.4);
      }
  
      .tool-card:hover .card-glass {
        filter: url('#liquid-refraction');
      }
  
      .tool-card:hover .card-glow {
        opacity: 1;
      }
  
      .t-icon, .t-meta {
        position: relative;
        z-index: 3;
      }
  
      .t-icon {
        font-size: 2.2rem;
        background: rgba(255, 255, 255, 0.02);
        padding: 12px;
        border-radius: 12px;
