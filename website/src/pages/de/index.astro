      .faq-section {
        padding: 0 7vw 100px;
      }
  
      .faq-content {
        max-width: 1000px;
        margin: 0 auto;
      }
  
      .faq-h2 {
        font-size: 2rem;
        font-weight: 800;
        margin-bottom: 48px;
